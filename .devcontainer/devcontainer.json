{
    "name": "Django DevContainer with Keycloak",
    "dockerComposeFile": [
      "../docker-compose.yml",
      "docker-compose.yml"
    ],
    "service": "django",
    "workspaceFolder": "/workspace",
    "extensions": [
      "ms-python.python",
      "ms-python.vscode-pylance"
    ],
    "settings": {
      "python.defaultInterpreterPath": "/usr/local/bin/python"
    },
    "postCreateCommand": "pip install -r requirements.txt",
    "remoteUser": "root"
  }
  